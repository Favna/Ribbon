{
    "name": "@favware/ribbon",
    "version": "9.0.0",
    "description": "feature rich, modular Discord.JS-Commando server bot",
    "author": "@favware",
    "license": "GPL-3.0",
    "private": true,
    "main": "./dist/app.js",
    "homepage": "https://favware.tech/ribbon",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/favna/ribbon.git"
    },
    "bugs": {
        "url": "https://github.com/favna/ribbon/issues"
    },
    "scripts": {
        "lint": "tslint -p . -c ./tslint.json --format verbose --fix",
        "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
        "prebuild": "yarn lint",
        "build": "tsc",
        "postbuild": "yarn copyfiles && yarn replace",
        "start": "cross-env NODE_ENV=development ts-node ./src/app.ts",
        "start:prod": "cross-env NODE_ENV=production ts-node ./src/app.ts",
        "start:js": "yarn build && cross-env NODE_ENV=development node ./dist/app.js",
        "serve": "pm2 start pm2.ecosystem.yml --env production",
        "serve:dev": "pm2 start pm2.ecosystem.yml",
        "copyfiles:fonts": "copyfiles -u 1 ./src/data/fonts/* ./dist",
        "copyfiles:databases": "copyfiles -u 1 ./src/data/databases/* ./dist",
        "copyfiles:env": "copyfiles -u 1 ./src/.env ./dist",
        "copyfiles:dex": "copyfiles -u 1 ./src/data/dex/*.json ./dist",
        "copyfiles": "concurrently \"yarn copyfiles:fonts\" \"yarn copyfiles:databases\" \"yarn copyfiles:env\" \"yarn copyfiles:dex\"",
        "replace:ts-flag": "replace --silent \"typescript: true,\" \"typescript: false,\" ./dist/Ribbon.js",
        "replace:commands-path": "replace --silent \".registerCommandsIn\\({\\n\\s+dirname.+\\n\\s+filter.+\\n\\s+}\\);\" \".registerCommandsIn(path_1.default.join(__dirname, 'commands'));\" ./dist/Ribbon.js",
        "replace:types-path": "replace --silent \".registerTypesIn\\({\\n\\s+dirname.+\\n\\s+filter.+\\n\\s+}\\)\" \".registerTypesIn(path_1.default.join(__dirname, 'components/commandoTypes'))\" ./dist/Ribbon.js",
        "replace": "yarn replace:ts-flag && yarn replace:commands-path && yarn replace:types-path",
        "predocs": "yarn docs:clean && yarn lint && yarn docs:build",
        "docs": "concurrently \"yarn docs:base\" \"yarn docs:wiki\" \"yarn docs:json\"",
        "postdocs": "yarn docs:clean",
        "docs:clean": "rimraf ./docsbuild",
        "docs:build": "tsc -p ./tsconfig.docs.json",
        "docs:base": "jsdoc2md --template ./docs/template.hbs --files ./docsbuild/commands/**/*.js --example-lang nginx > ./docs/index.md",
        "docs:wiki": "jsdoc2md --template ./docs/template.hbs --files ./docsbuild/commands/**/*.js --example-lang nginx > ../wikiribbon/All-Commands.md",
        "docs:json": "jsdoc2md --json --files ./docsbuild/commands/**/*.js > ../@favware-website/src/assets/docs/ribbon.json",
        "db:up": "docker-compose up -d",
        "db:stop": "docker-compose stop",
        "db:down": "docker-compose down",
        "prepublishOnly": "yarn build"
    },
    "keywords": [
        "bot",
        "Commando",
        "discord",
        "discord.js",
        "discord.js-commando",
        "Favna",
        "NodeJS",
        "TypeScript",
        "Ribbon"
    ],
    "engines": {
        "node": ">=8.0.0"
    },
    "dependencies": {
        "@favware/converter": "^1.0.2",
        "@favware/querystring": "^1.0.2",
        "@favware/unescape": "^1.0.1",
        "@favware/ytdl-prismplayer": "^1.0.0",
        "@favware/zalgo": "^1.0.1",
        "awesome-commando": "^3.0.9",
        "awesome-djs": "^13.0.16-beta.1",
        "better-sqlite3": "^5.4.0",
        "booru": "^2.0.2",
        "cheerio": "^1.0.0-rc.3",
        "common-tags": "^1.8.0",
        "didyoumean2": "^3.1.1",
        "dotenv": "^8.0.0",
        "emoji-regex": "^8.0.0",
        "erlpack": "^0.1.2",
        "fast-levenshtein": "^2.0.6",
        "firebase-admin": "^8.2.0",
        "fuse.js": "^3.4.5",
        "i18next": "^17.0.4",
        "interval-promise": "^1.3.0",
        "jimp": "^0.6.4",
        "libsodium-wrappers": "^0.7.4",
        "module-alias": "^2.2.0",
        "moment": "^2.24.0",
        "moment-duration-format": "^2.3.2",
        "node-fetch": "^2.6.0",
        "node-opus": "^0.3.2",
        "prism-media": "^1.0.2",
        "qrcode": "^1.3.3",
        "sagiri": "^1.5.3",
        "slot-machine": "^2.1.0",
        "speedtest-net": "^1.5.1",
        "table": "^5.4.1",
        "turndown": "^5.0.3",
        "zlib-sync": "^0.1.4"
    },
    "devDependencies": {
        "@favware/tslint-config": "^1.0.1",
        "@firebase/app-types": "^0.4.0",
        "@google-cloud/firestore": "^2.2.3",
        "@types/better-sqlite3": "^5.4.0",
        "@types/cheerio": "^0.22.11",
        "@types/common-tags": "^1.8.0",
        "@types/dotenv": "^6.1.1",
        "@types/fast-levenshtein": "^0.0.1",
        "@types/gulp": "^4.0.6",
        "@types/libsodium-wrappers": "^0.7.5",
        "@types/module-alias": "^2.0.0",
        "@types/moment-duration-format": "^2.2.2",
        "@types/node": "^12.0.10",
        "@types/node-fetch": "^2.3.7",
        "@types/qrcode": "^1.3.3",
        "@types/table": "^4.0.5",
        "@types/turndown": "^5.0.0",
        "@types/ws": "^6.0.1",
        "@types/yargs": "^13.0.0",
        "concurrently": "^4.1.1",
        "copyfiles": "^2.1.0",
        "cross-env": "^5.2.0",
        "gulp": "^4.0.2",
        "gulp-typescript": "^5.0.1",
        "jsdoc-to-markdown": "^5.0.0",
        "replace": "^1.1.0",
        "rimraf": "^2.6.3",
        "ts-node": "^8.3.0",
        "tslib": "^1.10.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.2",
        "yargs": "^13.2.4"
    }
}
