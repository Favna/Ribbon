{
    "name": "discord-ribbon",
    "version": "8.3.1",
    "description": "feature rich, modular Discord.JS-Commando server bot written in TypeScript",
    "author": "Jeroen \"Favna\" Claassens <support@favna.xyz> (https://favna.xyz/home)",
    "license": "GPL-3.0",
    "main": "./dist/app.js",
    "typings": "./dist/app.d.ts",
    "homepage": "https://favna.xyz/ribbon",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/favna/ribbon.git"
    },
    "bugs": {
        "url": "https://github.com/favna/ribbon/issues"
    },
    "scripts": {
        "test": "jest",
        "lint": "tslint --fix -p . -c ./tslint.json ./src/**/*.ts",
        "clean": "rimraf ./dist",
        "prebuild": "yarn clean && yarn lint && yarn test",
        "build": "tsc",
        "postbuild": "yarn copyfiles && yarn replace && yarn terser",
        "start": "cross-env NODE_ENV=development ts-node ./src/app.ts",
        "start:prod": "cross-env NODE_ENV=production ts-node ./src/app.ts",
        "serve": "pm2 start pm2.ecosystem.yml --env production",
        "serve:dev": "pm2 start pm2.ecosystem.yml",
        "copyfiles:fonts": "copyfiles -u 1 ./src/data/fonts/* ./dist",
        "copyfiles:databases": "copyfiles -u 1 ./src/data/databases/* ./dist",
        "copyfiles:env": "copyfiles -u 1 ./src/.env ./dist",
        "copyfiles:dex": "copyfiles -u 1 ./src/data/dex/*.json ./dist",
        "copyfiles": "concurrently \"yarn copyfiles:fonts\" \"yarn copyfiles:databases\" \"yarn copyfiles:env\" \"yarn copyfiles:dex\"",
        "replace:ts-flag": "replace --silent \"typescript: true,\" \"typescript: false,\" ./dist/Ribbon.js",
        "replace:commands-path": "replace --silent \".registerCommandsIn\\({\\n\\s+dirname.+\\n\\s+filter.+\\n\\s+}\\);\" \".registerCommandsIn(path_1.default.join(__dirname, 'commands'));\" ./dist/Ribbon.js",
        "replace:types-path": "replace --silent \".registerTypesIn\\({\\n\\s+dirname.+\\n\\s+filter.+\\n\\s+}\\)\" \".registerTypesIn(path_1.default.join(__dirname, 'components/commandoTypes'))\" ./dist/Ribbon.js",
        "replace": "yarn replace:ts-flag && yarn replace:commands-path && yarn replace:types-path",
        "predocs": "yarn docs:clean && yarn lint && yarn docs:build",
        "docs": "concurrently \"yarn docs:md\" \"yarn docs:json\"",
        "postdocs": "yarn docs:clean",
        "docs:clean": "rimraf ./docsbuild",
        "docs:build": "tsc -p ./tsconfig.docs.json",
        "docs:md": "jsdoc2md --template ./docs/template.hbs --files ./docsbuild/commands/**/*.js --example-lang nginx | tee ./docs/index.md > ../wikiribbon/All-Commands.md",
        "docs:json": "jsdoc2md --json --files ./docsbuild/commands/**/*.js > ../homesite/src/assets/docs/ribbon.json",
        "terser": "terser-folder ./dist -eo ./dist -x .js"
    },
    "keywords": [
        "bot",
        "Commando",
        "discord",
        "discord.js",
        "discord.js-commando",
        "Favna",
        "NodeJS",
        "TypeScript",
        "Ribbon"
    ],
    "engines": {
        "node": ">=10.0.0"
    },
    "dependencies": {
        "@discordjs/uws": "^11.149.1",
        "awesome-commando": "^2.1.1",
        "awesome-converter": "^1.0.3",
        "awesome-djs": "^13.0.4",
        "awesome-querystring": "^1.1.5",
        "awesome-zalgo": "^1.0.3",
        "better-sqlite3": "^5.4.0",
        "booru": "favna/booru",
        "cheerio": "^1.0.0-rc.2",
        "common-tags": "^1.8.0",
        "didyoumean2": "^2.0.2",
        "dotenv": "^6.1.0",
        "emoji-regex": "^8.0.0",
        "erlpack": "discordapp/erlpack",
        "fast-levenshtein": "^2.0.6",
        "fuse.js": "^3.4.4",
        "gulp-replace": "^1.0.0",
        "jimp": "^0.6.0",
        "libsodium-wrappers": "^0.7.3",
        "moment": "^2.24.0",
        "moment-duration-format": "^2.2.2",
        "nintendo-switch-eshop": "^2.1.1",
        "node-fetch": "^2.3.0",
        "node-opus": "^0.3.1",
        "prism-media": "^1.0.1",
        "qrcode": "^1.3.3",
        "slot-machine": "^2.1.0",
        "speedtest-net": "^1.5.1",
        "table": "^5.2.3",
        "unescape-es6": "^2.0.7",
        "ytdl-core": "^0.29.1",
        "zlib-sync": "^0.1.4"
    },
    "devDependencies": {
        "@babel/core": "^7.3.3",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-typescript": "^7.3.3",
        "@babel/register": "^7.0.0",
        "@types/better-sqlite3": "^5.2.2",
        "@types/cheerio": "^0.22.11",
        "@types/common-tags": "^1.8.0",
        "@types/dotenv": "^6.1.0",
        "@types/fast-levenshtein": "^0.0.1",
        "@types/jest": "^24.0.11",
        "@types/libsodium-wrappers": "^0.7.5",
        "@types/moment-duration-format": "^2.2.2",
        "@types/node": "^11.11.3",
        "@types/node-fetch": "^2.1.6",
        "@types/qrcode": "^1.3.1",
        "@types/table": "^4.0.5",
        "concurrently": "^4.1.0",
        "copyfiles": "^2.1.0",
        "cross-env": "^5.2.0",
        "gulp": "^4.0.0",
        "gulp-terser": "^1.1.7",
        "gulp-typescript": "^5.0.1",
        "jest": "^24.5.0",
        "jsdoc-to-markdown": "^4.0.1",
        "replace": "^1.1.0",
        "rimraf": "^2.6.3",
        "terser-folder": "^2.0.0",
        "ts-jest": "^24.0.0",
        "ts-node": "^8.0.3",
        "tslint": "^5.14.0",
        "typescript": "^3.3.3333",
        "yargs": "^13.2.2"
    },
    "files": [
        "dist/**/*.js",
        "dist/typings/*",
        "dist/**/*.json",
        "dist/data/fonts/*",
        "!dist/.env"
    ],
    "directories": {
        "doc": "docs/",
        "lib": "src/",
        "test": "test/"
    }
}
